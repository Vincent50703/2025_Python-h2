for i in range(1,3,1):
    print("i=",i)

print("印出 0 到 4：")
for i in range(5):
    print(i)

print("\n印出 1 到 10：")
for i in range(1, 11):
    print(i, end=" ")
print("\n\n印出 2 到 20 的偶數：")
for i in range(2, 21, 2):
    print(i, end=" ")

count = 1

while count <= 5:
    print(f"數字：{count}")
    count += 1  # 重要：更新計數器

print("迴圈結束")

